---
import Section from "./Section.astro";
import { speakersData } from "../data/speakers-data";
const speakers = Object.keys(speakersData);
import SpeakersDetail from "../components/SpeakersDetail.astro";

---

<Section 
  title="Speakers"
  id="speakers" 
  additionalClass="dots xl:min-h-screen" />
{
  speakers.map((speaker) => (
    <SpeakersDetail speakerId={speaker} showLink={true} />
  ))
}
